		<entry name="MoveSpeed"			type="int"					desc="이동 속도" />
		<entry name="AttackSpeed"		type="int"					desc="공격 속도" />
		<entry name="CastSpeed"			type="int"					desc="캐스팅 속도" />
		<entry name="HitRecovery"		type="int"					desc="피격 회복" />
