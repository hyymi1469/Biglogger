	</struct>

	<struct name="CurUserCountLogger" version="1" desc="실시간 접속자 로그(분당 1회)">
		<entry name="Time" 					type="datetime"	desc="YYYY-MM-DD HH:MM:SS" />
		<entry name="AppID"					type="string"	desc="APPID" />
		<entry name="UnixTime"          	type="int64"    desc="현재의 unixtime" />
		<entry name="Country"				type="string"   desc="서비스 국가" />
		<entry name="ServerIndex"      		type="string"   desc="서버 인덱스" />
		<entry name="WorldID"       		type="int"      desc="월드 id" />
		<entry name="OnlineCountIOS"		type="int"      desc="ios 접속자 수" />
		<entry name="OnlineCountAndroid"	type="int"      desc="android 접속자 수" />
	</struct>

	<struct name="FriendLogger" version="1" desc="친구">
		<entry name="ServerId"				type="string"				desc="서버 id" />
		<entry name="Time" 					type="datetime"				desc="YYYY-MM-DD HH:MM:SS" />
		<entry name="AppID"					type="string"				desc="APPID" />
		<entry name="PlatformID"			type="int"		default="0"	desc="ios 0 /android 1" />
		<entry name="AccountGrade"			type="int" 		default="0"	desc="계정 등급" />
		<entry name="AccountID"				type="string" 				desc="계정 고유 유니크 ID" />
		<entry name="UserID"				type="uint64" 				desc="유저 고유 유니크 ID" />
		<entry name="UserName"				type="string" 				desc="유저 닉네임" />
		<entry name="Level"					type="int"					desc="캐릭터 레벨" />
		<entry name="Type"					type="int"					desc="친구 타입" />
		<entry name="FriendUserID"			type="uint64"				desc="친구 캐릭터 고유 번호" />
		<entry name="FriendUserName"		type="string"				desc="친구 캐릭터 이름" />
