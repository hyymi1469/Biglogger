		<entry name="LoginTime"			type="int64"				desc="로그인 시간" />
		<entry name="NewCreateTime"		type="int64"				desc="캐릭터 생성 시간" />
		<entry name="NewDeleteTime"		type="int64"				desc="캐릭터 삭제 시간" />
